   "source": [
    "# Program to take screenshot\n",
    "\n",
    "import pyscreenshot\n",
    "\n",
    "# To capture the screen\n",
    "image = pyscreenshot.grab()\n",
    "\n",
    "# To display the captured screenshot\n",
    "image.show()\n",
    "\n",
    "# To save the screenshot\n",
    "image.save(\"GeeksforGeeks.png\")"
   ]
